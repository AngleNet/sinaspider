{
    "DOWNLOADER":{
        "link_batch_size": 100,
        "name_prefix": "downloader-test",
        "number_of_downloaders": 10
    },
    "LOGGER":{
        "use_terminal": true,
        "level": "DEBUG",
        "format": "[%(levelname)s]: %(asctime)s: %(filename)s:%(lineno)d %(name)s(%(processName)s): %(message)s",
        "datefmt": "%m-%d %H:%M:%S",
        "backup_window": 7
    },
    "PIPELINE":{
        "engine_pool_size": 2
    },
    "SCHEDULER_SERVICE":{
        "addr": "127.0.0.1",
        "port": "9999",
        "alive": true
    },
    "TEST":{
        "_comment": "Unit testing configures",
        "log_dir": "logs",
        "config":{
            "test_dummy1": 1,
            "test_dummy2": "test_dummy2",
            "test_dummy3": [1, 2, 3]
        },
        "scheduler_service":{
            "addr": "127.0.0.1",
            "port": 8000
        }
    }
}